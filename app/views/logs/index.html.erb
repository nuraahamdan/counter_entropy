<h1>Listing logs</h1>

<table>
  <tr>
    <th>Idname</th>
    <th>Ip</th>
    <th>Time</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.idname %></td>
    <td><%= log.ip %></td>
    <td><%= log.time %></td>
    <td><%= log.state %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
